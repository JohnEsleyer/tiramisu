{
    "name": "tiramisu-pro",
    "version": "2.0.0",
    "description": "Advanced video creation engine with WebCodecs, interactive editing, and AI-powered personalization",
    "type": "module",
    "main": "./index.ts",
    "types": "./index.ts",
    "exports": {
        ".": {
            "types": "./index.ts",
            "import": "./index.ts"
        },
        "./client": {
            "types": "./src/Client.ts",
            "import": "./src/Client.ts"
        },
        "./types": {
            "types": "./src/types.ts",
            "import": "./src/types.ts"
        }
    },
    "files": [
        "src",
        "index.ts",
        "README.md"
    ],
    "scripts": {
        "clean": "rm -rf dist",
        "build:wasm": "cd src/rust_audio && cargo build --target wasm32-unknown-unknown --release && cd ../.. && wasm-bindgen src/rust_audio/target/wasm32-unknown-unknown/release/tiramisu_audio_analyzer.wasm --out-dir ./src/wasm --target nodejs",
        "build": "bun run build:wasm",
        "dev": "bun run examples:serve",
        "examples:serve": "bun run examples/serve.ts",
        "examples:pro-editor": "bun run examples/serve.ts",
        "examples:marketing": "bun run examples/serve.ts",
        "examples:personalization": "bun run examples/serve.ts",
        "examples:social": "bun run examples/serve.ts"
    },
    "keywords": [
        "video",
        "canvas",
        "webcodecs",
        "ffmpeg",
        "puppeteer",
        "video-generation",
        "video-editing",
        "personalization",
        "marketing",
        "social-media"
    ],
    "peerDependencies": {
        "typescript": "^5.0.0"
    },
    "dependencies": {
        "puppeteer": "^24.36.1"
    },
    "devDependencies": {
        "@types/bun": "^1.3.8",
        "@types/node": "^22.0.0",
        "typescript": "^5.7.0"
    }
}
