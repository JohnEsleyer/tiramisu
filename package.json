{
  "name": "tiramisu",
  "version": "2.1.0",
  "main": "index.ts",
  "module": "index.ts",
  "types": "index.ts",
  "type": "module",
  "scripts": {
    "build:editor": "bun build ./examples/video-editor/app.ts --target browser --outfile ./examples/video-editor/editor-bundle.js --external \"../../src/Tiramisu\" --external \"../../src/Server\" --external \"../../src/Browser\" --external \"../../src/Encoder\" --external \"../../src/CLI\" --external \"../../src/AudioAnalysis\" --external \"puppeteer\"",
    "dev:editor": "bun run build:editor && bun run examples/serve.ts",
    "build:upload": "bun build ./examples/video-upload/app.ts --target browser --outfile ./examples/video-upload/bundle.js --external \"../../src/Tiramisu\" --external \"../../src/Server\" --external \"../../src/Browser\" --external \"../../src/Encoder\" --external \"../../src/CLI\" --external \"../../src/AudioAnalysis\" --external \"puppeteer\"",
    "dev:upload": "bun run build:upload && bun run examples/serve.ts"
  },
  "dependencies": {
    "puppeteer": "^24.36.1"
  },
  "devDependencies": {
    "@types/bun": "^1.3.8",
    "@types/node": "^25.2.0"
  }
}